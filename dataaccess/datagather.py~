
import threading
import time

class DataGather(threading.Thread):
    """
    Query data provider engine
    """

    RESULT = {'exchange'   : None,
              'instrument' : None}
        
    def __init__(self):
        super(DataGather, self).__init__()
        
    def run(self):
        print "Running...", self.name
        time.sleep(5)
        self.RESULT['result'] = "Resulting"

    
